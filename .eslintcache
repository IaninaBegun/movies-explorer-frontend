viesCard from '../MoviesCard/MoviesCard';\nimport './MoviesCardList.css';\n\nfunction MoviesCardList ( {\n    isSavedMovie,\n    foundMovies,\n    onSavedMovie,\n    onDeleteMovie,\n    isCurrentlySaved\n  } ) {\n\n  /*console.log(foundMovies);*/\n\n  const location = useLocation();\n  const moviesLocation = location.pathname === '/movies';\n  const savedMoviesLocation = location.pathname === '/saved-movies';\n\n\n  const isNotFound = foundMovies && foundMovies.length > 0 ? true : false;\n\n  /*movies, функция получения числа отображаемых изначально после поиска карточек */\n  const getMoviesNumber = () => {\n    if (window.innerWidth >= 900) {\n      return 12;\n    }\n    if (window.innerWidth >= 768) {\n      return 8;\n    }\n    if (window.innerWidth >= 300) {\n      return 5;\n    }\n  }\n\n  const [ moviesToRender, setMoviesToRender ] = React.useState(getMoviesNumber());\n\n  /*movies, функция получения числа карточек при клике на ещё */\n  const getMoviesToShowNumber = () => {\n    if (window.innerWidth >= 900) {\n      return 3;\n    } else {\n      return 2;\n    }\n  }\n\n  React.useEffect(() => {\n    setTimeout(() => {\n     window.addEventListener('resize', () => getMoviesNumber);\n    }, 300);\n  }, []);\n\n  const handleShowMoreMovies = () => {\n    setMoviesToRender(moviesToRender + getMoviesToShowNumber());\n  };\n\n  const moviesToDisplay = !isSavedMovie && foundMovies ? foundMovies.slice(0, moviesToRender) : foundMovies;\n\n\n  return (\n    <section className=\"movies page__section\">\n      {!isNotFound || !foundMovies ? ( `Ничего не найдено`) :\n\n        (<ul className=\"movies__list\">\n\n        {moviesToDisplay.map((movie) => {\n          const movieKey = movie.movieId;\n            return(\n              <MoviesCard\n                key={movieKey}\n                movie={movie}\n                isSavedMovie={isSavedMovie}\n                foundMovies={foundMovies}\n                onSavedMovie={onSavedMovie}\n                onDeleteMovie={onDeleteMovie}\n                isCurrentlySaved={isCurrentlySaved}\n              />\n            );\n        })}\n\n      </ul>)\n    }\n\n      <button\n        type=\"submit\"\n        className={ !foundMovies || (!isSavedMovie && moviesToRender < foundMovies.length) ? `movies__btn` : `movies__btn_invsible`}\n        onClick={() => handleShowMoreMovies()}\n      >Ещё</button>\n\n    </section>\n  )\n}\n\nexport default MoviesCardList;\n","C:\\Users\\Сергей\\Desktop\\YandexPraktium\\movies-explorer-frontend\\src\\utils\\constants.js",[],["138","139"],"C:\\Users\\Сергей\\Desktop\\YandexPraktium\\movies-explorer-frontend\\src\\components\\MoviesCard\\MoviesCard.js",[],"C:\\Users\\Сергей\\Desktop\\YandexPraktium\\movies-explorer-frontend\\src\\components\\Register\\Register.js",[],"C:\\Users\\Сергей\\Desktop\\YandexPraktium\\movies-explorer-frontend\\src\\components\\Login\\Login.js",[],"C:\\Users\\Сергей\\Desktop\\YandexPraktium\\movies-explorer-frontend\\src\\components\\Header\\Header.js",[],"C:\\Users\\Сергей\\Desktop\\YandexPraktium\\movies-explorer-frontend\\src\\components\\Navigation\\Navigation.js",[],"C:\\Users\\Сергей\\Desktop\\YandexPraktium\\movies-explorer-frontend\\src\\components\\Profile\\Profile.js",[],"C:\\Users\\Сергей\\Desktop\\YandexPraktium\\movies-explorer-frontend\\src\\components\\NavigationMainPage\\NavigationMainPage.js",[],"C:\\Users\\Сергей\\Desktop\\YandexPraktium\\movies-explorer-frontend\\src\\components\\PageNotFound\\PageNotFound.js",[],"C:\\Users\\Сергей\\Desktop\\YandexPraktium\\movies-explorer-frontend\\src\\components\\PopupErrors\\PopupErrors.js",[],"C:\\Users\\Сергей\\Desktop\\YandexPraktium\\movies-explorer-frontend\\src\\components\\Footer\\Footer.js",[],"C:\\Users\\Сергей\\Desktop\\YandexPraktium\\movies-explorer-frontend\\src\\contexts\\CurrentUserContext.js",[],"C:\\Users\\Сергей\\Desktop\\YandexPraktium\\movies-explorer-frontend\\src\\utils\\MoviesApi.js",[],"C:\\Users\\Сергей\\Desktop\\YandexPraktium\\movies-explorer